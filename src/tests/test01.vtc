varnishtest "Test dgram vmod"

server s1 {
    rxreq
    txresp
} -start

varnish v1 -vcl+backend {
    import dgram from "${vmod_topbuild}/src/.libs/libvmod_dgram.so";

    sub vcl_deliver {
        dgram.send_dgram("hi", "127.0.0.1", 12345);
    }
} -start

client c1 {
    txreq -url "/"
    rxresp
}

client c1 -run
